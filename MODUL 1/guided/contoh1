#include <iostream>
using namespace std;

int main() {
    int angka1 = 10;
    int angka2 = 20;

    if (angka1 > angka2) {
        cout << "angka1 lebih besar dari angka2" << endl;
    } else if (angka1 < angka2) {
        cout << "angka1 lebih kecil dari angka2" << endl;
    } else {
        cout << "angka1 sama dengan angka2" << endl;
    } 
    return 0;
}
